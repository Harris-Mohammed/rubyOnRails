<div class="container">
<h1> Add New Book</h1>

<%= form_for :book, url: books_path do |f| %>
<div class="form-group">
<%= f.label :title %>
<%= f.text_field :title %>
</div>

<div class="form-group">
<%= f.label :price %>
<%= f.number_field :price %>
</div>

<div class="form-group">
<%= f.label :bookid %>
<%= f.number_field :bookid %>
</div>

<div class="form-group">
<%= f.label :Publisher %>
<%= f.text_field :publisher %>
</div>

<div class="form-group">
<%= f.label :description %>
<%= f.text_field :description %>
</div>

<div class="form-group">
<%= f.label :number %>
<%= f.number_field :number %>
</div>

<div class="form-group">
<%= f.submit %>
</div>
<% end %>
</div>